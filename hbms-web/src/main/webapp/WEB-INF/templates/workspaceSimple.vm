    <style>
        body {
            font-family: '微软雅黑';
            color: #666;
            min-width: 1210px;
        }
        .navbar {
            min-height: 50px;
        }
        .navbar .navbar-nav > li > a {
            padding-top: 0px;
            padding-bottom: 0px;
            margin-top: 0px;
            margin-bottom: 0px;
            line-height: 50px;
            min-height: 50px;
            font-size: 16px;
        }

        .navbar-brand {
            height: 100px;
            padding-top: 10px;
        }
        .navbar-toggle {
            margin-top: 4px;
            margin-bottom: 4px;
        }

        .navbar-default .navbar-nav > li > a:hover,
        .navbar-default .navbar-nav > li > a:focus {
            color: red;
        }

        body,html{margin: 0px;padding: 0;width: 100%;background-color: white}

        .navbar{
          width:100%;
          margin:0px;
          position:fixed;/*固定作用*/
          top:0px;
          left:0px;
          /*ie6下样式，加下划线表示只针对ie6 的hack */
          _position:absolute;/* 把导航栏位置定义为绝对位置  关键*/
          _top:expression(documentElement.scrollTop + "px"); /* 把导航栏位置放在浏览器垂直滚动条的顶端  关键 */
          z-index:9999; /* 让导航栏浮在网页的高层位置，遇到flash和图片时候也能始终保持最外层 */
          text-align:left;
        }

        .workspace {
          padding: 5;
          margin: 5;
        }

    </style>

