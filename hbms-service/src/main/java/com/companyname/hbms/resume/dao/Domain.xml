<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="domain">

  <resultMap id="Resume" type="com.companyname.hbms.resume.domain.Resume">
    <id property="id" column="id" jdbcType="INTEGER"/>
    <result property="candidateId" column="candidateId" jdbcType="INTEGER"/>
    <result property="name" column="name" jdbcType="VARCHAR"/>
    <result property="keyword" column="keyword" jdbcType="VARCHAR"/>
    <result property="attacheUri" column="attacheUri" jdbcType="VARCHAR"/>
    <result property="languageId" column="languageId" jdbcType="INTEGER"/>
    <result property="yn" column="yn" jdbcType="INTEGER"/>
    <result property="createTime" column="createTime" jdbcType="DATE"/>
    <result property="createUser" column="createUser" jdbcType="VARCHAR"/>
    <result property="updateTime" column="updateTime" jdbcType="DATE"/>
    <result property="updateUser" column="updateUser" jdbcType="VARCHAR"/>
    <association property="candidate" columnPrefix="TAL_" resultMap="Candidate"/>
  </resultMap>

  <resultMap id="ResumeReport" type="com.companyname.hbms.resume.domain.ResumeReport">
    <id property="id" column="id" jdbcType="INTEGER"/>
    <result property="candidateId" column="candidateId" jdbcType="INTEGER"/>
    <result property="name" column="name" jdbcType="VARCHAR"/>
    <result property="keyword" column="keyword" jdbcType="VARCHAR"/>
    <result property="attacheUri" column="path" jdbcType="VARCHAR"/>
    <result property="companyId" column="languageId" jdbcType="INTEGER"/>
    <result property="positionId" column="languageId" jdbcType="INTEGER"/>
    <result property="projectId" column="languageId" jdbcType="INTEGER"/>
    <result property="languageId" column="languageId" jdbcType="INTEGER"/>
    <result property="type" column="type" jdbcType="INTEGER"/>
    <result property="yn" column="yn" jdbcType="INTEGER"/>
    <result property="createTime" column="createTime" jdbcType="DATE"/>
    <result property="createUser" column="createUser" jdbcType="VARCHAR"/>
    <result property="updateTime" column="updateTime" jdbcType="DATE"/>
    <result property="updateUser" column="updateUser" jdbcType="VARCHAR"/>
    <association property="candidate" columnPrefix="TAL_" resultMap="Candidate"/>
    <association property="company" columnPrefix="COM_" resultMap="domain.Company"/>
    <association property="position" columnPrefix="POS_" resultMap="domain.Position"/>
    <association property="project" columnPrefix="PRJ_" resultMap="domain.Project"/>
  </resultMap>

</mapper>